{
    "name": "iota_client",
    "version": "0.0.1",
    "keywords": "crypto, iota, client",
    "description": "IOTA C client library",
    "repository": {
        "type": "git",
        "url": "https://github.com/oopsmonk/iota.c.git",
        "branch": "pio_mbed"
    },
    "authors": {
        "name": "Sam Chen",
        "email": "sam.chen@iota.org",
        "maintainer": true
    },
    "frameworks": "mbed",
    "platforms": "*",
    "build":{
        "srcFilter":[
            "+<cclient/request/*.[ch]>",
            "+<cclient/response/*.[ch]>",
            "+<cclient/serialization/*.[ch]>",
            "-<cclient/serialization/json/tests/*>"
        ]
    },
    "dependencies": [
        {
            "name": "iota_common",
            "version": "https://github.com/oopsmonk/iota_common.git#pio_lib"
        },
        {
            "name": "http_parser",
            "version": "https://github.com/nodejs/http-parser.git#v2.9.2"
        },
        {
            "name": "cJSON",
            "version": "https://github.com/DaveGamble/cJSON.git#v1.7.12"
        }
    ]
}
